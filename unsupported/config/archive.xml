<?xml version="1.0" encoding="UTF-8"?>
<dmz>
<plugins>
   <plugin name="dmzArchivePluginObject"/>
   <plugin name="dmzObjectModuleBasic"/>
   <plugin name="dmzObjectPluginDump"/>
</plugins>
<runtime>
   <types>
      <object name="vehicle">
      </object>
   </types>
   <states>
      <state name="Damaged"/>
      <state name="Dead"/>
   </states>
</runtime>
<dmzArchivePluginObject>
   <object type="vehicle">
      <attributes>
         <state value="Damaged"/>
         <position x="0.0" y="0.0" z="0.0"/>
         <orientation v0="1.0" v1="0.0" v2="0.0" v3="0.0" v4="1.0" v5="0.0" v6="0.0" v7="0.0" v8="1.0"/>
         <!--
         NOTE: euler specifies orientation in euler angles. Use <euler> or <orientation>
               but not both!
         <euler x="0.0" y="0.0" z="0.0"/>
         -->
         <velocity x="0.0" y="0.0" z="0.0"/>
         <acceleration x="0.0" y="0.0" z="0.0"/>
         <scale x="0.0" y="0.0" z="0.0"/>
         <vector x="0.0" y="0.0" z="0.0"/>
         <scalar value="0.0"/>
         <text value="This is a text value"/>
      </attributes>
   </object>
</dmzArchivePluginObject>
</dmz>
